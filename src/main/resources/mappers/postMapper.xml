<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.memo.post.dao.PostMapper">

	<select resultType="map" id="selectPostList">select * from `post` </select>

	<select resultType="com.memo.post.domain.Post"
		id="selectPostListByUserId" parameterType="int">SELECT
		`id`,`userId`,`subject`,`content`,`imagePath`,`createdAt`,`updatedAt`FROM
		`post`WHERE `userId` = #{userId} order by id desc </select>

</mapper>